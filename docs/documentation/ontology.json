}, {
  "@id" : "http://www.w3id.org/framECO#Circumstance",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Circumstances or conditions under which the Experiencer experiences the emotion. Unlike Content it does not give the specific content of the emotion, but the Content can often be inferred from the Circumstances."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Circumstance"
  } ],
  "http://www.w3.org/2002/07/owl#disjointWith" : [ {
    "@id" : "http://www.w3id.org/framECO#Content"
  }, {
    "@id" : "http://www.w3id.org/framECO#Stimulus"
  } ]
}, {
  "@id" : "http://www.w3id.org/framECO#Content",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Content is what the Experiencer's feelings or experiences are directed towards or based upon. The Content differs from a stimulus because the Content is not construed as being directly responsible for causing the emotion."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Content"
  } ],
  "http://www.w3.org/2002/07/owl#disjointWith" : [ {
    "@id" : "http://www.w3id.org/framECO#Stimulus"
  } ]
}, {
