    "@id" : "_:genid2"
  } ]
}, {
  "@id" : "http://www.w3id.org/framECO#Entity",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Any living being"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Entity"
  } ]
}, {
  "@id" : "http://www.w3id.org/framECO#Event",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The Event is the occasion or happening that Experiencers in a certain emotional state participate in."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Event"
  } ]
}, {
