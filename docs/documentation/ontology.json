  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A Physiological Process is a Bodily Process which is an Expressor of an Experiencer's Emotion. It can be a change in the rate of breathing, muscle becoming tense or relaxing, etc."
  }, {
    "@language" : "en",
    "@value" : "A Physiological Process is a Bodily Process, which is an Expressor of an Experiencer's Emotion. It can be a change in the rate of the heart beating, or muscle relaxing or becoming tense, or crying etc."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Physiological Process"
