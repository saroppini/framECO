    "instances" : 0,
    "label" : {
      "IRI-based" : "Experiencer",
      "en" : "Experiencer"
    },
    "comment" : {
      "en" : "The Experiencer is the person or sentient entity that experiences or feels the Emotion or other internal state such as Mood."
    },
    "id" : "3",
    "superClasses" : [ "2" ]
  }, {
