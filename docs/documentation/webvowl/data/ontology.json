    "instances" : 0,
    "label" : {
      "IRI-based" : "Circumstance",
      "en" : "Circumstance"
    },
    "comment" : {
      "en" : "Circumstances or conditions under which the Experiencer experiences the emotion. Unlike Content it does not give the specific content of the emotion, but the Content can often be inferred from the Circumstances."
    },
    "id" : "6"
  }, {
    "iri" : "http://www.w3id.org/framECO#HighDegree",
