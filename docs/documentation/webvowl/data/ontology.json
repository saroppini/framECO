    "label" : {
      "IRI-based" : "Emotion",
      "en" : "Emotion"
    },
    "intersection" : [ "5" ],
    "comment" : {
      "en" : "An emotion is an Affective State lived by an Experiencer which involves or is evoked by specific Circumstances. For this reason it is usually intense and short-lived."
    },
    "attributes" : [ "intersection" ],
    "id" : "4",
    "superClasses" : [ "5" ]
  }, {
    "iri" : "http://www.w3id.org/framECO#Circumstance",
